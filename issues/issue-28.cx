package issue28

type Structure struct {
	string str
}

func (structure Structure) function () (out str) {
	out = structure.string
}

package main
import "issue28"
import "os"
import "cx"

func main () {
	var structure issue28.Structure
	structure.string = "abc"
	if (assert(structure.string, "abc", "") == false) {
		os.Exit(cx.INTERNAL_ERROR)
	}

	var out str
	out = structure.function()
	if (assert(out, "abc", "") == false) {
		os.Exit(cx.INTERNAL_ERROR)
	}
}
