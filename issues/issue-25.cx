package main

import "cx"
import "os"

type issue25Type struct {
	i i32
	ai [5]i32
}

func main()(error i32) {
	var too issue25Type
	too.i = 3
	printf("too.i %d\n", too.i)
	too.ai = [5]i32{ 666, 1, 2, 3, 4 }
	printf("too.i %d\n", too.i)
	if (assert(too.i, 3, "field overwritten by array") == false) {
		os.Exit(cx.INTERNAL_ERROR)
	}
}
