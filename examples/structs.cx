package main

type Point struct {
	x i32
	y i32
}

func printPoint (p Point) () {
	str.print("From function")
	i32.print(p.y)
}

func returnStruct () (p Point) {
	var p Point
	p.x = 66
	p.y = 33
}

func main () (out i32) {
	var uPoint Point
	i32.print(uPoint.x)
	i32.print(uPoint.y)

	var myPoint Point
	myPoint.x = i32.add(5, 10)
	myPoint.y = i32.add(myPoint.x, 20)

	i32.print(i32.add(myPoint.x, myPoint.y))

	printPoint(myPoint)

	ret := returnStruct()
	i32.print(ret.x)
	i32.print(ret.y)
}
