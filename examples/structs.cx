package main

type Point struct {
	name str
	x i32
	y i32
}

func main () (out i32) {
	var uPoint Point;
	str.print(uPoint.name)
	i32.print(uPoint.x)
	i32.print(uPoint.y)

	var myPoint Point
	myPoint.x = i32.add(5, 10)
	myPoint.y = i32.add(myPoint.x, 20)

	i32.print(i32.add(myPoint.x, myPoint.y))
}
