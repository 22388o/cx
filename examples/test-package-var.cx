package main

var jj i32

func main() () {
	testNestedForGlobalVar()
}

func testNestedForGlobalVar() () {
	str.print("Running NESTED FOR with global var tests...")
	var i i32
	for i = 0; i32.lteq(i, 10); i = i32.add(i, 1){
		for jj = 1; i32.lteq(jj, 10); jj = i32.add(jj, 1){
			if i32.eq(i32.mod(i, jj), 0) {
				str.print("Even")
				//str.printf("i: %v, jj: %v\n", i, jj)
			} else {
				str.print("Odd")
				//str.printf("i: %v, jj: %v\n", i, jj)
			}
		}
	}
}
