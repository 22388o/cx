package foo1

func double (n i32) (res i32) {
	res = i32.add(n, n)
}

package foo2

func triple (n i32) (res i32) {
	res = i32.mul(n, 3)
}

package main
import "foo1"
import "foo2"

func main () {
	var double i32
	var triple i32
	
	double = foo1.double(10)
	triple = foo2.triple(10)

	i32.print(double)
	i32.print(triple)
}

