package main

var option1 str = "access granted"
var option2 str = "access denied"

// basic usage of affordances
func main () () {
	objs := ->{ age $18.0 }

	rules := ->{
		if age $18.0 {
			allow(x == x)
			// allow(x == "access granted")
			// allow(x == "access denied")
		}
	}
	target := ->{
		pkg(main) fn(main) exp(message)
	}

	affs := aff.query(target, objs, rules)
	[]str.print(affs)
	aff.print(affs)
	aff.execute(target, affs, 1)

message:
	str.print("")
}
