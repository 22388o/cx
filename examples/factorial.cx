package main

func factorial (num i64) (fact i64) {
	fact = 0
	if i64.eq(num, 1L) {
		fact = 1L
	} else {
		fact = i64.mul(num, factorial(i64.sub(num, 1L)))
	}
}

func main () () {
	var start i64
	var end i64

	start = time.UnixNano()
	i64.print(factorial(20L))
	end = time.UnixNano()

	str.print("Elapsed time in calculation:")
	i64.print(i64.sub(end, start))
}
