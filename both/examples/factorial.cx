package main

func factorial (num i32) (fact i32) {
	if i32.eq(num, 1) {
		fact = 1
	} else {
		fact = i32.mul(num, factorial(i32.sub(num, 1)))
	}
}

func main () () {
	// i32.print(factorial(6))
	// i32.print(factorial(5))
	// i32.print(factorial(6))
	i32.print(factorial(7))
	i32.print(factorial(5))
}
