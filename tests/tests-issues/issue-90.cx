package issues

func issue90 () {
	var cmd str
beginning:
	cmd = "P"
	
	if cmd == "C" {
		goto cat
	}

	if cmd == "D" {
		goto dog
	}

	if cmd == "P" {
		goto pigeon
	}

cat:
	issues = append(issues, assert(cmd, "C", ""))
	cmd = "E"
	goto end
dog:
	issues = append(issues, assert(cmd, "D", ""))
	cmd = "C"
	goto cat
pigeon:
	issues = append(issues, assert(cmd, "P", ""))
	cmd = "D"
	goto dog

end:
	issues = append(issues, assert(cmd, "E", ""))
}
