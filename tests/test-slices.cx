package main

func main () {
	var slc []i32
	slc = append(slc, 11)
	slc = append(slc, 12)
	slc = append(slc, 13)
	slc = append(slc, 14)
	slc = append(slc, 15)
	slc = append(slc, 16)

	assert_(len(slc), 6, "")

	assert_(slc[0], 11, "")
	assert_(slc[1], 12, "")
	assert_(slc[2], 13, "")
	assert_(slc[3], 14, "")
	assert_(slc[4], 15, "")
	assert_(slc[5], 16, "")

	var slc2 []i32
	slc2 = slc

	assert_(slc2[0], 11, "")
	assert_(slc2[1], 12, "")
	assert_(slc2[2], 13, "")
	assert_(slc2[3], 14, "")
	assert_(slc2[4], 15, "")
	assert_(slc2[5], 16, "")

	assert_(len(slc2), 6, "")

	var slc3 []i32
	slc3 = []i32{21, 22, 23, 24, 25}

	assert_(slc3[0], 21, "")
	assert_(slc3[1], 22, "")
	assert_(slc3[2], 23, "")
	assert_(slc3[3], 24, "")
	assert_(slc3[4], 25, "")

	assert_(len(slc3), 5, "")

	var slc4 []i32
	slc4 = append(slc3, 26)

	assert_(slc4[0], 21, "")
	assert_(slc4[1], 22, "")
	assert_(slc4[2], 23, "")
	assert_(slc4[3], 24, "")
	assert_(slc4[4], 25, "")
	assert_(slc4[5], 26, "")

	assert_(len(slc4), 6, "")

	var num i32
	num = slc4[5]

	assert_(num, 26, "")
	assert_(num, slc4[5], "")

	var slc5 []f32
	slc5 = append(slc5, 1.1)
	slc5 = append(slc5, 1.2)
	slc5 = append(slc5, 1.3)

	assert_(slc5[0], 1.1, "")
	assert_(slc5[1], 1.2, "")
	assert_(slc5[2], 1.3, "")

	assert_(len(slc5), 3, "")

	slc5 = []f32{11.11, 12.12, 13.13, 14.14}

	assert_(slc5[0], 11.11, "")
	assert_(slc5[1], 12.12, "")
	assert_(slc5[2], 13.13, "")
	assert_(slc5[3], 14.14, "")

	assert_(len(slc5), 4, "")

	var slc6 []i64
	slc6 = append(slc6, 11L)
	slc6 = append(slc6, 12L)
	slc6 = append(slc6, 13L)

	assert_(slc6[0], 11L, "")
	assert_(slc6[1], 12L, "")
	assert_(slc6[2], 13L, "")

	slc6 = []i64{11L, 12L, 13L, 14L}

	assert_(slc6[0], 11L, "")
	assert_(slc6[1], 12L, "")
	assert_(slc6[2], 13L, "")
	assert_(slc6[3], 14L, "")

	var slc7 []f64
	slc7 = append(slc7, 11.11D)
	slc7 = append(slc7, 12.12D)
	slc7 = append(slc7, 13.13D)

	assert_(slc7[0], 11.11D, "")
	assert_(slc7[1], 12.12D, "")
	assert_(slc7[2], 13.13D, "")

	slc7 = []f64{11.11D, 12.12D, 13.13D, 14.14D}

	assert_(slc7[0], 11.11D, "")
	assert_(slc7[1], 12.12D, "")
	assert_(slc7[2], 13.13D, "")
	assert_(slc7[3], 14.14D, "")

	var slc8 []bool
	slc8 = append(slc8, true)
	slc8 = append(slc8, false)
	slc8 = append(slc8, true)

	assert_(slc8[0], true, "")
	assert_(slc8[1], false, "")
	assert_(slc8[2], true, "")

	slc8 = []bool{false, true, true, true, false, true}

	assert_(slc8[0], false, "")
	assert_(slc8[1], true, "")
	assert_(slc8[2], true, "")
	assert_(slc8[3], true, "")
	assert_(slc8[4], false, "")
	assert_(slc8[5], true, "")

	var slc10 []bool
	slc10 = append(slc10, true)
	slc10 = append(slc10, false)
	slc10 = append(slc10, true)

	assert_(slc10[0], true, "")
	assert_(slc10[1], false, "")
	assert_(slc10[2], true, "")

	slc10 = []bool{false, true, true, true, false, true}

	assert_(slc10[0], false, "")
	assert_(slc10[1], true, "")
	assert_(slc10[2], true, "")
	assert_(slc10[3], true, "")
	assert_(slc10[4], false, "")
	assert_(slc10[5], true, "")

	var slc9 []byte
	slc9 = append(slc9, 15B)
	slc9 = append(slc9, 20B)
	slc9 = append(slc9, 35B)

	assert_(slc9[0], 15B, "")
	assert_(slc9[1], 20B, "")
	assert_(slc9[2], 35B, "")

	slc9 = []byte{90B, 95B, 100B, 105B, 110B, 115B}

	assert_(slc9[0], 90B, "")
	assert_(slc9[1], 95B, "")
	assert_(slc9[2], 100B, "")
	assert_(slc9[3], 105B, "")
	assert_(slc9[4], 110B, "")
	assert_(slc9[5], 115B, "")

	var slc11 []str
	slc11 = append(slc11, "hello world", "")
	slc11 = append(slc11, "bye oh cruel world", "")

	assert_(slc11[0], "hello world", "")
	assert_(slc11[1], "bye oh cruel world", "")

	var slc12 []str
	slc12 = append(slc11, "1", "")
	slc12 = append(slc12, "22", "")

	assert_(slc12[0], "hello world", "")
	assert_(slc12[1], "bye oh cruel world", "")
	assert_(slc12[2], "1", "")
	assert_(slc12[3], "22", "")
}
