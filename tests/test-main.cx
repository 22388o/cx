package main
import "testing"
import "time"

func main () {
	printf("\nRunning CX tests\n")
	
	var start i64
	start = time.UnixNano()
	
	testing.testI32()
	testing.testI64()
	testing.testF32()
	testing.testF64()
	testing.testBOOL()
	testing.testArray()
	testing.testFunctions()
	

	testing.testControlFlow()
	testing.testStructures()

	testing.testSTR()
	testing.testPointers()
	testing.testSlices()
	testing.testShortDeclarations()
	testing.testParse()
	testing.collectionFunctions()
	// testing.testBYTE()

	var end i64
	end = time.UnixNano()

	printf("Tests finished after %d nanoseconds (%f milliseconds)\n\n", i64.sub(end, start), f64.div(i64.f64(end-start), 1000000.0D))
}
