package main
import "testing"

func main () {
	var start i64
	start = time.UnixNano()
	
	testing.testI32()
	testing.testI64()
	testing.testF32()
	testing.testF64()
	testing.testBOOL()
	testing.testArray()
	testing.testFunctions()

	testing.testControlFlow()
	testing.testStructures()

	testing.testSTR()
	testing.testPointers()
	testing.testSlices()
