package main
import "os"
import "cx"
type issue85struct struct {
	suit i32
	rank i32
}

func (c issue85struct) issue85method1 (suit i32, rank i32) {
	c.suit = suit
	c.rank = rank
}

func (c *issue85struct) issue85method2 (suit i32, rank i32) {
	(*c).suit = suit
	(*c).rank = rank
}

func main () {
	var card issue85struct

	assert_(card.suit, 0, "")
	assert_(card.rank, 0, "")

	card.issue85method1(7, 11)

	assert_(card.suit, 0, "")
	assert_(card.rank, 0, "")

	card.issue85method2(7, 11)

	assert_(card.suit, 7, "")
	assert_(card.rank, 11, "")
}
