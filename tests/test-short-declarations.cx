package main

type Point struct {
	name str
	isTrue bool
	x i32
	y i32
}

func main () {
	byt1 := 44B
	byt2 := 47B
	assert_(byt1, 44B, "")
	assert_(byt2, 47B, "")

	bool1 := true
	bool2 := false
	assert_(bool1, true, "")
	assert_(bool2, false, "")

	num1 := 5
	num2 := 10
	assert_(num1, 5, "")
	assert_(num2, 10, "")

	num3 := 33L
	num4 := 55L
	assert_(num3, 33L, "")
	assert_(num4, 55L, "")

	dec1 := 5.0
	dec2 := 10.0
	assert_(dec1, 5.0, "")
	assert_(dec2, 10.0, "")

	dec3 := 33.0D
	dec4 := 55.0D
	assert_(dec3, 33.0D, "")
	assert_(dec4, 55.0D, "")

	p1 := Point{
		x: 10,
		y: 11,
		isTrue: true,
		name: "Pointy"}
	p2 := Point{
		x: 20,
		y: 21,
		isTrue: false,
		name: "Punto"}

	assert_(p1.name, "Pointy", "")
	assert_(p1.x, 10, "")
	assert_(p1.y, 11, "")
	assert_(p1.isTrue, true, "")

	assert_(p2.name, "Punto", "")
	assert_(p2.x, 20, "")
	assert_(p2.y, 21, "")
	assert_(p2.isTrue, false, "")
}
