package main

func main () {
	assert_(byte.str(10B), "10", "")
	assert_(byte.i32(10B), 10, "")
	assert_(byte.i64(10B), 10L, "")
	assert_(byte.f32(10B), 10.0, "")
	assert_(byte.f64(10B), 10.0D, "")

	assert_(i32.str(10), "10", "")
	assert_(i32.i64(10), 10L, "")
	assert_(i32.f32(10), 10.0, "")
	assert_(i32.f64(10), 10.0D, "")
	assert_(i32.byte(379), 123B, "")

	assert_(i64.i32(10L), 10, "")
	assert_(i64.str(10L), "10", "")
	assert_(i64.i64(10L), 10L, "")
	assert_(i64.f32(10L), 10.0, "")
	assert_(i64.f64(10L), 10.0D, "")
	assert_(i64.byte(379L), 123B, "")

	assert_(f32.byte(10.0), 10B, "")
	assert_(f32.str(10.5), "10.5", "")
	assert_(f32.i32(10.0), 10, "")
	assert_(f32.i64(10.0), 10L, "")
	assert_(f32.f64(10.0), 10.0D, "")

	assert_(f64.byte(10.0D), 10B, "")
	assert_(f64.str(10.5D), "10.5", "")
	assert_(f64.i32(10.0D), 10, "")
	assert_(f64.i64(10.0D), 10L, "")
	assert_(f64.f32(10.0D), 10.0, "")

	assert_(str.byte("0"), 0B, "")
	assert_(str.i32("123"), 123, "")
	assert_(str.i64("444"), 444L, "")
	assert_(str.f32("333"), 333.0, "")
	assert_(str.f64("333"), 333.0D, "")
}
